<?xml version="1.0" encoding="UTF-8"?>
<!--https://spotbugs.readthedocs.io/en/stable/filter.html-->
<FindBugsFilter>
    <Match>
        <Bug pattern="DLS_DEAD_LOCAL_STORE"/>
        <Class name="com.jumia.skylens.http.in.controllers.UserController"/>
        <Method name="testApi" params="authentication.com.jumia.skylens.http.in.acl.AuthToken" returns="com.jumia.skylens.http.in.responses.authentication.UserResponse"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
        <Or>
            <Class name="interceptors.com.jumia.skylens.http.in.WebHeadersLogContextImpl"/>
            <Class name="com.jumia.skylens.kafka.in.KafkaProcessorContext"/>

            <Package name="com.jumia.skylens.app.configurations.properties.*"/>
            <Package name="com.jumia.skylens.commons.logging.*"/>
        </Or>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/>
        <Or>
            <Class name="interceptors.com.jumia.skylens.http.in.WebHeadersLogContextImpl"/>
            <Class name="com.jumia.skylens.kafka.in.KafkaProcessorContext"/>

            <Package name="com.jumia.skylens.domain.impl.*"/>
            <Package name="com.jumia.skylens.app.configurations.kafka.*"/>
            <Package name="com.jumia.skylens.app.configurations.properties.*"/>
            <Package name="com.jumia.skylens.commons.logging.*"/>
        </Or>
    </Match>
    <Match>
        <Or>
            <Package name="com.jumia.skylens.http.in.model.*"/>
            <Source name="~.*?ConverterImpl.*"/>
        </Or>
    </Match>
</FindBugsFilter>
